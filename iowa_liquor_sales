--The following pertains to the dataset iowa_liquor_sales from the bigquery-public-data dataset.


--Report: address of all liquor stores in Iowa by zip code

SELECT
  store_number,
  store_name,
  address,
  city,
  zip_code,
  store_location,
  county
FROM
  `bigquery-public-data.iowa_liquor_sales.sales`
ORDER BY
  zip_code ASC
  
  
--Report: single store's sales in single year

SELECT
  store_number,
  store_name,
  SUM(sale_dollars)
FROM
  `bigquery-public-data.iowa_liquor_sales.sales`
WHERE
  store_number = '4509'
  AND date <'2022-01-01' AND date >'2020-12-31'
GROUP BY store_number, store_name
